              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[stg_productos_G01]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    CAST(PRODUCT_ID AS INT) AS PRODUCT_ID,
    DETAIL,
    PACKAGE,
    UPPER(REVERSE(SUBSTRING(REVERSE(TRIM(DETAIL)), 1, CHARINDEX(' ', REVERSE(TRIM(DETAIL))) - 1))) AS PRODUCT_TYPE,
    CAST(SUBSTRING(PACKAGE,1, CHARINDEX(' ', PACKAGE) - 1) AS DECIMAL(10,3)) AS VOLUME,
    CASE
        WHEN PACKAGE LIKE '%cm3%' THEN 'CM3'
        WHEN PACKAGE LIKE '%Liter%' THEN 'LITER'
        ELSE 'Unknown'
    END AS UNIT_OF_MEASURE,
    CAST(SUBSTRING(PACKAGE, 1, CHARINDEX(' ', PACKAGE) - 1) AS DECIMAL(10,3))
    * CASE 
        WHEN PACKAGE LIKE '%cm3%' THEN 0.001 
        ELSE 1 
    END AS AMOUNT_LITER,
    CASE 
        WHEN LOWER(PACKAGE) LIKE '%can%' THEN 1 
        ELSE 0 
    END AS IS_CAN
FROM [bd_staging_2025_G01].[dbo].[stg_productos_G01]
ORDER BY PRODUCT_ID;</property>
