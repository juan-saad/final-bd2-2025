                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with
  TMP_Fact
  as
  (
          SELECT
        t1.BILLING_ID,
        t1.DATE,
        t1.CUSTOMER_ID,
        t1.EMPLOYEE_ID,
        t2.PRODUCT_ID,
        t2.QUANTITY,
        t1.REGION
      FROM [bd_staging_2025_G01].[dbo].[stg_ventas_G01] as t1
        INNER JOIN [bd_staging_2025_G01].dbo.[stg_detalle_ventas_G01] as t2 on t1.BILLING_ID = t2.BILLING_ID

    UNION ALL

      SELECT
        t1.BILLING_ID,
        t1.DATE,
        t1.CUSTOMER_ID,
        t1.EMPLOYEE_ID,
        t1.PRODUCT_ID,
        t1.QUANTITY,
        t1.REGION
      FROM [bd_staging_2025_G01].[dbo].[stg_ventas_hist_G01] t1
  ),
  TMP_Precios
  as
  (
    SELECT t1.PRODUCT_ID
      , t1.DATE
      , LEAD(DATE, 1) OVER (PARTITION BY PRODUCT_ID ORDER BY DATE) AS [UNTIL]
      , t1.PRICE
    FROM [bd_staging_2025_G01].[dbo].[stg_precios_G01] t1
  ),
  CTE_Total_Facturado
  AS
  (
    select v.billing_id, v.date, SUM(v.quantity) * SUM(p.PRICE) AS total_facturado
    from TMP_Fact v
      left join TMP_Precios p
      on v.product_id = p.PRODUCT_ID
        and v.date &gt;= p.[DATE]
        and (v.date &lt; p.UNTIL or p.UNTIL is null)
    group by v.billing_id, v.date
  ),
  CTE_Descuentos
  AS
  (
    select v.billing_id, v.date, v.total_facturado, isnull(MAX(d.PERCENTAGE/100.00),0.00) AS descuento
    from CTE_Total_Facturado v --1656457
      left join [bd_staging_2025_G01].[dbo].[stg_descuentos_G01] d
      on v.date &gt;= d.[FROM]
        and (v.date &lt; d.UNTIL or d.UNTIL is null)
        and v.total_facturado &gt; d.TOTAL_BILLING
    group by v.billing_id, v.date, v.total_facturado
  )
select 
  v.BILLING_ID AS [VENTA_ID]
  , v.DATE AS [FECHA_VENTA]
  , v.CUSTOMER_ID AS [CLIENTE_ID]
  , v.EMPLOYEE_ID AS [EMPLEADO_ID]
  , v.PRODUCT_ID AS [PRODUCTO_ID]
  , v.QUANTITY AS [CANTIDAD]
  , v.REGION AS [REGION]
  , v.quantity * p.PRICE AS [TOTAL_FACTURADO]
  , v.quantity * p.PRICE * (1-d.descuento)  AS [TOTAL_FACTURADO_CON_DESCUENTO]
  , p.PRICE AS [PRECIO]
  , d.descuento AS [DESCUENTO]
from TMP_Fact v
  left join TMP_Precios p
  on v.product_id = p.PRODUCT_ID
    and v.date &gt;= p.[DATE]
    and (v.date &lt; p.UNTIL or p.UNTIL is null)
  left join CTE_Descuentos d
  on v.billing_id = d.billing_id
ORDER BY v.BILLING_ID</property>
